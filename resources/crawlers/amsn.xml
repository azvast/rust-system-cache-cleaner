<crawler id="cleaner">
    <name>amsn</name>              
    <info> </info>              
    <options id="cache">        
        <info>Deletes the cache</info>          
        <run cmd="delete" find="glob" upath="~/.amsn/*/displaypic/cache/*.dat" />
        <run cmd="delete" find="glob" upath="~/.amsn/*/displaypic/cache/*.png" />
        
        <!-- uncomment to delete chat
        <run cmd="delete" find="glob" upath="~/.amsn/*/logs/date" />
        <run cmd="delete" find="glob" upath="~/.amsn/*/logs/*.log" /> -->
        
    </options>
</crawler>