<crawler id="cleaner">
    <name>Internet Explorer</name>              
    <info>Microsofts built in browser</info>              
    <options id="cache">        
        <info>Deletes the cache</info>          
        <run cmd="delete" find="walk_files" path="%USERPROFILE%\Cookies\" />
        <run cmd="delete" find="walk_files" path="%APPDATA%\Microsoft\Windows\Cookies\" />
        <run cmd="delete" find="walk_files" path="%localappdata%\Microsoft\Internet Explorer\DOMStore\" />
        <run cmd="delete" find="walk_files" path="%USERPROFILE%\AppData\LocalLow\Microsoft\Internet Explorer\DOMStore\" />
        <run cmd="delete" find="walk_files" path="%APPDATA%\Microsoft\Windows\IETldCache\" />
        <run cmd="delete" find="walk_files" path="%USERPROFILE%\AppData\Local\Microsoft\Windows\Temporary Internet Files\" />
        <run cmd="delete" find="walk_files" path="%USERPROFILE%\Local Settings\Temporary Internet Files\" />
        <run cmd="delete" find="walk_files" path="%localappdata%\Microsoft\Feeds Cache\" />
    </options>
</crawler>